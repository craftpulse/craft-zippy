{
    "name": "percipiolondon/craft-zippy",
    "description": "Zip multiple assets",
    "type": "craft-plugin",
    "version": "4.0.0-beta.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "zippy"
    ],
    "support": {
        "docs": "https://github.com/percipioglobal/craft-zippy/blob/master/README.md",
        "issues": "https://github.com/percipioglobal/craft-zippy/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Percipio London",
            "homepage": "https://percipio.london"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms":"^4.0.0-alpha"
    },
    "require-dev": {
        "craftcms/phpstan":"dev-main"
    },
    "autoload": {
        "psr-4": {
            "percipiolondon\\zippy\\": "src/"
        }
    },
    "scripts": {
        "phpstan": "phpstan --memory-limit=1G"
    },
    "extra": {
        "name": "Zippy",
        "handle": "zippy",
        "developer": "percipiolondon",
        "developerUrl": "https://percipio.london",
        "documentationUrl": "https://github.com/percipioglobal/craft-zippy/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/percipioglobal/craft-zippy/master/CHANGELOG.md",
        "class": "percipiolondon\\zippy\\Zippy"
    }
}
